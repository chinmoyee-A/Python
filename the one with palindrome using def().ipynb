    "def reverse(str1):\n",
    "    if(len(str1)==0):\n",
    "        return str1\n",
    "    else:\n",
    "        return(reverse(str1[1:])+str1[0])\n",
    "\n",
    "string=input(\"enter the string : \")\n",
    "str1=reverse(string)\n",
    "print(\"reverse of the string is : \",str1)\n",
    "if(string==str1):\n",
    "    print(\"yes\")\n",
    "else:\n",
    "    print(\"no\")"
